package com.example.generics;

public class DataHolder<T,V> {

	T data;
	V size;

	public DataHolder(T data) {
		this.data = data;
	}

	public DataHolder(T data,V size) {
		this.data = data;
		this.size = size;
	}

	public void display() {
		System.out.println(this.data);
	}
	
	public T read() {
		return this.data;
	}
}
