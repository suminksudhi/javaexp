<!DOCTYPE html>
<html>
<head>
<script>

	var d  = new Date();
	var str  = new String("Fine");
	var arr  = new Array(45,76,2,3,4,6,7);
	console.log(d);
	console.log(str);
	console.log(arr);
	
	var bye = new Function("document.write('Bye Mr. Raj');");
	alert(typeof bye);
	
	bye();
	
	var i = 100;
	
	alert(i);
	alert(typeof i);
	
	var hi = function (){
		document.write("Hi Mr. Raj");
		document.write("<br>");
	}
	
	alert(hi);
	alert(typeof hi);
	
	hi();
	
	function hello(){
		document.write("Hello Mr. Raj");
		document.write("<br>");
	}

	hello();
	alert(typeof hello);
	
</script>
