package com.example.generics;

public class Account {

	int id;
	String name;
	double amount;
	boolean status;

	public Account() {
	}

	public Account(String name, double amount, boolean status) {
		this.name = name;
		this.amount = amount;
		this.status = status;
	}


	public Account(int id, String name, double amount, boolean status) {
		this.id = id;
		this.name = name;
		this.amount = amount;
		this.status = status;
	}


	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public double getAmount() {
		return amount;
	}

	public void setAmount(double amount) {
		this.amount = amount;
	}

	public boolean isStatus() {
		return status;
	}

	public void setStatus(boolean status) {
		this.status = status;
	}

	@Override
	public String toString() {
		return "Account [id=" + id + ", amount=" + amount + ", status=" + status + "]";
	}

}
