package com.example.enumex;

public enum Season {

	WINTER,SUMMER,SPRING,RAINY;

	int month[] = new int[3];
	
	private Season(){
		System.out.println("Inside Season()");
	}
	
	private Season(int m1,int m2,int m3){
		System.out.println("Inside Season(int m1,int m2,int m3)");
		month[0] = m1;
		month[1] = m2;
		month[2] = m3;
	}
}
