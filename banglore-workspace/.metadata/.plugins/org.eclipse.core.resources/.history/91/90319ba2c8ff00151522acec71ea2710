package com.example.generics;

public class DataHolderDemo {
	
	public static void main(String[] args) {
		new DataHolderDemo();
	}
	
	public DataHolderDemo() {
		demoTwo();
		//demoOne();
	}

	private void demoTwo(){
		DataHolder<String> holder = new DataHolder<String>("Cat");
		holder.display();

		DataHolder<Integer> holder = new DataHolder<String>("Cat");
		holder.display();
	}
	
	private void demoOne(){
		DataHolder holder1 = new DataHolder("Lion");
		holder1.display();

		DataHolder holder2 = new DataHolder(120000);
		holder2.display();
	}
}
